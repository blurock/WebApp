<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
	<m:MaterialCard ui:field="card">
		<m:MaterialCardContent>
		<m:MaterialCardTitle ui:field="cardtitle" iconType="POLYMER" iconPosition="RIGHT"/>
		<m:MaterialLabel  ui:field="cardlabel" />
		<m:MaterialCollection>
		<m:MaterialCollectionItem>
				<m:MaterialLink ui:field="activate" text="Activate"
					iconType="LABEL" />
			</m:MaterialCollectionItem>
			<m:MaterialCollectionItem>
				<m:MaterialLink ui:field="login" text="First Login"
					iconType="DONE" />
			</m:MaterialCollectionItem>
			<m:MaterialCollectionItem>
				<m:MaterialLink ui:field="profile" text="Profile"
					iconType="PERSON" />
			</m:MaterialCollectionItem>
			<m:MaterialCollectionItem>
				<m:MaterialLink ui:field="mainPage" text="Home"
					iconType="HOME" />
			</m:MaterialCollectionItem>
			</m:MaterialCollection>
			</m:MaterialCardContent>
		</m:MaterialCard>
	</g:HTMLPanel>
</ui:UiBinder> 