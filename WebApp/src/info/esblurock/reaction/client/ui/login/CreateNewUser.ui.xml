<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui" 
	xmlns:user="urn:import:info.esblurock.reaction.client.panel.description">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="contentpanel">
			<m:MaterialRow>
				<m:MaterialColumn grid="s5">
					<m:MaterialLabel ui:field="settitle" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s3">
					<m:MaterialButton ui:field="submitdata" textColor="blue-text text-darken-2 light-blue lighten-5">
					</m:MaterialButton>
				</m:MaterialColumn>
			</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialTextBox ui:field="username" />
					</m:MaterialColumn>
				</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialTextBox ui:field="emailaddress" />
					</m:MaterialColumn>
				</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialTextBox ui:field="password" type="PASSWORD"/>
					</m:MaterialColumn>
				</m:MaterialRow>
				<m:MaterialRow>
					<m:MaterialColumn grid="s12">
						<m:MaterialTextBox ui:field="repeatpassword" type="PASSWORD"/>
					</m:MaterialColumn>
				</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 