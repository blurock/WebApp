<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='info.esblurock.reaction.client.ui.resource.MaterialResources' />
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	header, main, footer {
		padding-left: 320px;
	}
	</ui:style>
	<m:MaterialNavBar type="FIXED">
			<m:MaterialLink iconType="LIST" textColor="white"/>
		<m:MaterialSideNav width="300" m:id="mysidebar" type="FIXED" closeOnClick="false">
			<m:MaterialTopNav
				 ui:field="topNav" textColor="white">
					<m:MaterialLink text="edward.blurock@esblurock.info"
						textColor="white" fontSize="0.8"
						iconPosition="RIGHT" />
						<m:MaterialImage resource="{res.blackboardbackground}"/>
			</m:MaterialTopNav>
			<m:MaterialCollapsible>
					<m:MaterialCollapsibleItem>
						<m:MaterialCollapsibleHeader>
							<m:MaterialLink text="Contact Input"
								textColor="blue" />
						</m:MaterialCollapsibleHeader>
						<m:MaterialCollapsibleBody>
							<m:MaterialRow>
								<m:MaterialColumn>
									<m:MaterialLink ui:field="inputorganization"
										iconPosition="LEFT" text="Organization" textColor="blue" />
								</m:MaterialColumn>
							</m:MaterialRow>
							<m:MaterialRow>
								<m:MaterialColumn>
									<m:MaterialLink ui:field="inputcontact"
										iconPosition="LEFT" text="User" textColor="blue" />
								</m:MaterialColumn>
							</m:MaterialRow>
						</m:MaterialCollapsibleBody>
					</m:MaterialCollapsibleItem>
					<m:MaterialCollapsibleItem>
						<m:MaterialCollapsibleHeader>
							<m:MaterialLink
								text="Data Input" textColor="blue"  />
						</m:MaterialCollapsibleHeader>
						<m:MaterialCollapsibleBody>
							<m:MaterialCollapsible>

								<m:MaterialCollapsibleItem>
									<m:MaterialCollapsibleHeader>
										<m:MaterialLink 
											text="CHEMKIN" textColor="blue"/>
									</m:MaterialCollapsibleHeader>
									<m:MaterialCollapsibleBody>
										<m:MaterialRow>
											<m:MaterialColumn>
												<m:MaterialLink ui:field="chemkin"
													iconPosition="LEFT" textColor="blue"  />
											</m:MaterialColumn>
										</m:MaterialRow>
										<m:MaterialRow>
											<m:MaterialColumn>
												<m:MaterialLink ui:field="nasapolynomials"
													iconPosition="LEFT" />
											</m:MaterialColumn>
										</m:MaterialRow>
									</m:MaterialCollapsibleBody>
								</m:MaterialCollapsibleItem>
								<m:MaterialCollapsibleItem>
									<m:MaterialCollapsibleHeader>
										<m:MaterialLink 
											text="2D graphical information" textColor="blue"/>
									</m:MaterialCollapsibleHeader>
									<m:MaterialCollapsibleBody>
										<m:MaterialRow>
											<m:MaterialColumn>
												<m:MaterialLink ui:field="sdfmolecules" textColor="blue" />
											</m:MaterialColumn>
										</m:MaterialRow>
										<m:MaterialRow>
											<m:MaterialColumn>
												<m:MaterialLink ui:field="sdfsubstructures"  textColor="blue" />
											</m:MaterialColumn>
										</m:MaterialRow>
										<m:MaterialRow>
											<m:MaterialColumn>
												<m:MaterialLink ui:field="thergasmolecules"
													iconPosition="LEFT" textColor="blue" />
											</m:MaterialColumn>
										</m:MaterialRow>
									</m:MaterialCollapsibleBody>
								</m:MaterialCollapsibleItem>
							</m:MaterialCollapsible>
						</m:MaterialCollapsibleBody>
					</m:MaterialCollapsibleItem>
					<m:MaterialCollapsibleItem>
						<m:MaterialCollapsibleHeader>
							<m:MaterialLink 
								text="Transactions" textColor="blue"/>
						</m:MaterialCollapsibleHeader>
						<m:MaterialCollapsibleBody>
							<m:MaterialRow>
								<m:MaterialColumn>
									<m:MaterialLink ui:field="uploadsets"
										iconPosition="LEFT" text="File Sets" textColor="blue" />
								</m:MaterialColumn>
							</m:MaterialRow>
							<m:MaterialRow>
								<m:MaterialColumn>
									<m:MaterialLink ui:field="toprocess"
										iconPosition="LEFT" text="To Process" textColor="blue"/>
								</m:MaterialColumn>
							</m:MaterialRow>
							<m:MaterialRow>
								<m:MaterialColumn>
									<m:MaterialLink ui:field="transactions"
										iconPosition="LEFT" text="Transactions" textColor="blue" />
								</m:MaterialColumn>
							</m:MaterialRow>
							<m:MaterialRow>
								<m:MaterialColumn>
								</m:MaterialColumn>
							</m:MaterialRow>
						</m:MaterialCollapsibleBody>
					</m:MaterialCollapsibleItem>
			</m:MaterialCollapsible>
		</m:MaterialSideNav>
	</m:MaterialNavBar>
</ui:UiBinder> 