<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" 
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='res'
		type='info.esblurock.reaction.client.ui.resource.MaterialResources' />
	<ui:style>
	.important {
		font-weight: bold;
	}
	
	header, main, footer {
		padding-left: 320px;
	}
	</ui:style>
	<m:MaterialNavBar wave="light" align="right" color="blue"
		text="Starter" sideBar="fixed" sideBarWidth="300">
		<m:nav>
		<m:MaterialLink icon="mdi-action-list" textColor="white"
				tooltip="Other Windows" />
		</m:nav>
		<m:sidebaritem>
			<m:MaterialTopNav profileName="Data Input and Query"
			resource="{res.blackboardbackground}"
				ui:field="topNav" textColor="white" type="sidebar">
				<m:child>
					<m:MaterialLink text="edward.blurock@esblurock.info"
						textColor="white" fontSize="0.8" icon="mdi-navigation-arrow-drop-down"
						iconPosition="right" />
				</m:child>
			</m:MaterialTopNav>
		</m:sidebaritem>
		<m:sidebaritem>
			<m:MaterialLink ui:field="inputquery" icon="mdi-action-search"
				iconPosition="left" text="Query" textColor="blue"
				tooltip="keyword query of database" tooltipLocation="right" />
		</m:sidebaritem>
		<m:sidebaritem>
			<m:MaterialCollapsible>
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink text="Contact Input" icon="mdi-content-add-circle"
								textColor="blue"
								tooltip="Input contact information for people and organizations"
								tooltipLocation="right" />
						</m:header>
						<m:content>
							<m:UnorderedList>
								<m:ListItem>
									<m:MaterialLink ui:field="inputorganization"
										iconPosition="left" text="Organization" textColor="blue"
										tooltip="Input organization description" tooltipLocation="right" />
								</m:ListItem>
								<m:ListItem>
									<m:MaterialLink ui:field="inputcontact"
										iconPosition="left" text="User" textColor="blue"
										tooltip="Input user description" tooltipLocation="right" />
								</m:ListItem>
							</m:UnorderedList>
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>
				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink icon="mdi-content-add-circle"
								text="Data Input" textColor="blue"
								tooltip="Enter data in database" tooltipLocation="right" />
						</m:header>
						<m:content>
							<m:UnorderedList>
								<m:ListItem>
									<m:MaterialLink ui:field="chemkin" iconPosition="left"
										textColor="blue"
										tooltipLocation="right" />
								</m:ListItem>
								<m:ListItem>
									<m:MaterialLink ui:field="sdfmolecules"
										iconPosition="left"
										tooltipLocation="right"
										textColor="blue" />
								</m:ListItem>
								<m:ListItem>
									<m:MaterialLink ui:field="sdfsubstructures"
										iconPosition="left"
										tooltipLocation="right"
										textColor="blue" />
								</m:ListItem>
								<m:ListItem>
									<m:MaterialLink ui:field="thergasmolecules"
										iconPosition="left" 
										tooltipLocation="right"
										textColor="blue" />
								</m:ListItem>
								<m:ListItem>
									<m:MaterialLink ui:field="nasapolynomials"
										iconPosition="left"
										tooltipLocation="right"
										textColor="blue" />
								</m:ListItem>
							</m:UnorderedList>
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>

				<m:item>
					<m:MaterialCollapsibleItem>
						<m:header>
							<m:MaterialLink icon="mdi-content-add-circle"
								text="Transactions" textColor="blue"
								tooltip="Transactions to process and have been processed" tooltipLocation="right" />
						</m:header>
						<m:content>
							<m:UnorderedList>
								<m:ListItem>
									<m:MaterialLink ui:field="uploadsets" iconPosition="left"
										text="File Sets" textColor="blue"
										tooltip="File sets to process"
										tooltipLocation="right" />
								</m:ListItem>
								<m:ListItem>
									<m:MaterialLink ui:field="toprocess" iconPosition="left"
										text="To Process" textColor="blue"
										tooltip="Transactions to process"
										tooltipLocation="right" />
								</m:ListItem>
								<m:ListItem>
									<m:MaterialLink ui:field="transactions" iconPosition="left"
										text="Transactions" textColor="blue"
										tooltip="Transactions"
										tooltipLocation="right" />
								</m:ListItem>
							</m:UnorderedList>
						</m:content>
					</m:MaterialCollapsibleItem>
				</m:item>



			</m:MaterialCollapsible>
		</m:sidebaritem>
	</m:MaterialNavBar>
</ui:UiBinder> 