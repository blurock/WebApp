<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel" xmlns:gwt="urn:import:gwtupload.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem ui:field="repositorytop">
		<m:MaterialCollapsibleHeader>
			<m:MaterialRow>
				<m:MaterialColumn grid="s10">
					<m:MaterialLink ui:field="refresh" iconType="AUTORENEW"
						iconColor="grey" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s10">
					<m:MaterialLink ui:field="stringlabel" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
			<g:HTMLPanel ui:field="setpanel">
				<m:MaterialCollapsible ui:field="repository" />
			</g:HTMLPanel>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>

</ui:UiBinder>