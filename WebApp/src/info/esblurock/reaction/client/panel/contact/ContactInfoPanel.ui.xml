<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
			<m:MaterialCollapsibleItem>
				<m:MaterialCollapsibleHeader>
					<m:MaterialLink ui:field="organizationcontact" text="Organization Contact"/>
				</m:MaterialCollapsibleHeader>
				<m:MaterialCollapsibleBody>
					<g:HTMLPanel>
						<m:MaterialRow>
							<m:MaterialColumn grid="s12 m6">
								<m:MaterialTextBox ui:field="email"
									ui:text="first.last@mail.com" 
									placeholder="contact email" />
							</m:MaterialColumn>
							<m:MaterialColumn grid="s12 m6">
								<m:MaterialTextBox ui:field="phone" 
									ui:text="(555) 55-5555"
									placeholder="contact phone" />
							</m:MaterialColumn>
						</m:MaterialRow>
						<m:MaterialRow>
							<m:MaterialColumn grid="s4 m4">
								<m:MaterialTextBox 
									ui:field="mainhomepage"
									text="http://www.toppage.com" 
									placeholder="homepage" />
							</m:MaterialColumn>
						</m:MaterialRow>
					</g:HTMLPanel>
				</m:MaterialCollapsibleBody>
			</m:MaterialCollapsibleItem>
</ui:UiBinder> 