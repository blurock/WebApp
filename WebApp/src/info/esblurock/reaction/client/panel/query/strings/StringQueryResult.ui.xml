<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel" xmlns:gwt="urn:import:gwtupload.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	.customlink {
		font-size: 0.9em;
		icon-size: TINY;
	}
	.customicon {
		font-size: 0.9em;
		icon-size: TINY;
	}
	</ui:style>
	<m:MaterialCollapsibleItem ui:field="item">
		<m:header>
			<g:HTMLPanel>
				<m:MaterialRow>
					<m:MaterialColumn grid="s1">
						<m:MaterialLink ui:field="actions" styleName="{style.customlink}">
						<m:MaterialIcon  ui:field="actionicon" icon="mdi-navigation-arrow-forward" styleName="{style.customicon}"/> 
						</m:MaterialLink>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s3">
						<m:MaterialLink  ui:field="predicate" styleName="{style.customlink} "/>
					</m:MaterialColumn>
					<m:MaterialColumn grid="s8">
						<m:MaterialLink ui:field="textarea" styleName="{style.customlink}" />
					</m:MaterialColumn>
				</m:MaterialRow>
			</g:HTMLPanel>
		</m:header>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 