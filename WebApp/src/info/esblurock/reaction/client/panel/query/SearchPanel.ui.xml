<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:ma="urn:import:gwt.material.design.addins.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
	<m:MaterialModal ui:field="objectinfomodal" type="FIXED_FOOTER" dismissable="false" inDuration="500" outDuration="800">
	<m:MaterialModalContent ui:field="infocontent">
	</m:MaterialModalContent>
	<m:MaterialModalFooter>
			<m:MaterialButton ui:field="objectclose" waves="GREEN"
				type="FLAT" text="Close" />
			<m:MaterialButton ui:field="objectsave" waves="GREEN"
				type="FLAT" text="Close" />
	</m:MaterialModalFooter>
	</m:MaterialModal>
	<m:MaterialRow>
	<m:MaterialColumn backgroundColor="blue lighten-3">
	<m:MaterialLink ui:field="link" grid="s1" iconColor="black" backgroundColor="blue lighten-3"/>
	</m:MaterialColumn>
	<m:MaterialColumn>
	<g:HTMLPanel>
	
	<m:MaterialLink ui:field="expandnext" grid="s1"  iconColor="black" backgroundColor="blue lighten-3"/>
	<m:MaterialLink ui:field="objectinfo" grid="s1"  iconColor="black" backgroundColor="blue lighten-3"/>
	</g:HTMLPanel>
	</m:MaterialColumn>
	<m:MaterialColumn>
	<m:MaterialLink ui:field="searchText" grid="s10" backgroundColor="blue lighten-3" textColor="black"/>	
	</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialLink ui:field="pathtext" grid="s12" backgroundColor="blue lighten-3" textColor="black"/>
	<m:MaterialRow>
	<g:HTMLPanel ui:field="resultpanel" />
	</m:MaterialRow>
	</g:HTMLPanel>
</ui:UiBinder> 