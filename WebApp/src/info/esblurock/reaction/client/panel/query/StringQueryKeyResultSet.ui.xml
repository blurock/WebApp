<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:m="urn:import:gwt.material.design.client.ui" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel" xmlns:gwt="urn:import:gwtupload.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem ui:field="item">
		<m:header>
			<m:MaterialRow>
				<m:MaterialColumn grid="s1">
					<m:MaterialLink ui:field="remove" icon="mdi-navigation-close" />
				</m:MaterialColumn>
				<m:MaterialColumn grid="s11">
					<m:MaterialLink ui:field="label" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:header>
		<m:content>
			<g:HTMLPanel>
				<m:MaterialCollapsible ui:field="set"/>
			</g:HTMLPanel>
		</m:content>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 