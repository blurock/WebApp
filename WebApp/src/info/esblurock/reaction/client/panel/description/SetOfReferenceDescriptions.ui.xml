<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialCollapsibleItem>
		<m:MaterialCollapsibleHeader
			backgroundColor="cyan lighten-5">
			<m:MaterialLink ui:field="referenceheader" text="References"
				iconType="CREATE" iconPosition="LEFT" textColor="blue" />
		</m:MaterialCollapsibleHeader>
		<m:MaterialCollapsibleBody>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12">
					<m:MaterialLink ui:field="addreference" text="Add Reference"
						textColor="blue" iconType="CREATE" iconPosition="LEFT" />
				</m:MaterialColumn>
			</m:MaterialRow>
			<m:MaterialRow>
				<m:MaterialColumn grid="s12">
					<m:MaterialCollapsible ui:field="references" />
				</m:MaterialColumn>
			</m:MaterialRow>
		</m:MaterialCollapsibleBody>
	</m:MaterialCollapsibleItem>
</ui:UiBinder> 