<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:user="urn:import:info.esblurock.reaction.client.panel.description">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="contentpanel">
	<m:MaterialModal ui:field="modal" type="DEFAULT"  dismissable="true" inDuration="500" outDuration="800">
     <m:MaterialModalContent ui:field="modalcontent">
	</m:MaterialModalContent>
     <m:MaterialModalFooter>
         <m:MaterialButton text="Close Modal" type="FLAT"/>
     </m:MaterialModalFooter>
	</m:MaterialModal>
		<m:MaterialRow>
			<m:MaterialColumn grid="s5">
				<m:MaterialLabel ui:field="settitle" />
			</m:MaterialColumn>
			<m:MaterialColumn grid="s3">
				<m:MaterialButton ui:field="submitdata"
					textColor="blue-text text-darken-2 light-blue lighten-5">
				</m:MaterialButton>
			</m:MaterialColumn>
		</m:MaterialRow>
		<m:MaterialCollapsible ui:field="collapsable">
		</m:MaterialCollapsible>
	</g:HTMLPanel>
</ui:UiBinder> 